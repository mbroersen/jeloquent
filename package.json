{
  "name": "jeloquent",
  "module": "dist/jeloquent.js",
  "unpkg": "dist/jeloquent.js",
  "main": "dist/jeloquent.js",
  "version": "3.1.1",
  "description": "micro framework / memory orm model store",
  "scripts": {
    "build": "./node_modules/.bin/esbuild ./src/Jeloquent.js --bundle --format=esm --platform=browser --minify --keep-names --outfile=./dist/jeloquent.js --loader:.js=jsx",
    "watch": "./node_modules/.bin/esbuild ./src/Jeloquent.js --bundle --format=esm --platform=browser --minify --keep-names --outfile=./dist/jeloquent.js --loader:.js=jsx --watch",
    "test": "jest",
    "test-coverage": "jest --coverage=true",
    "lint": "eslint src/** --ext .js,.jsx,.ts,.tsx --fix-dry-run",
    "lint-fix": "eslint src/** --ext .js,.jsx,.ts,.tsx --fix"
  },
  "keywords": [
    "eloquent",
    "framework",
    "models",
    "orm",
    "datastorage",
    "memory",
    "store",
    "laravel",
    "data"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbroersen/jeloquent.git"
  },
  "bugs": {
    "url": "https://github.com/mbroersen/jeloquent/issues"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/mbroersen"
  },
  "homepage": "https://github.com/mbroersen/jeloquent#readme",
  "author": "Mark Broersen",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.8.1",
    "esbuild": "^0.14.11",
    "esbuild-jest": "^0.5.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "typescript": "^4.5.4"
  }
}
